{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "regions": ["gru1", "cdg1", "sfo1"],
  "alias": "tripby.org",
  "routes": [
    { "src": "/substance/(?<name>[^/]+)", "dest": "/substance?name=$name" },
    {
      "src": "/psicoativos/cafeina",
      "status": 301,
      "headers": { "Location": "/substance?name=Caffeine" }
    },
    {
      "src": "/psicoativos/cocaina",
      "status": 301,
      "headers": { "Location": "/substance?name=Cocaine" }
    },
    {
      "src": "/psicoativos/cogumelos-psicodelicos",
      "status": 301,
      "headers": {
        "Location": "/substance?name=Psilocybin"
      }
    },
    {
      "src": "/psicoativos/dxm",
      "status": 301,
      "headers": { "Location": "/substance?name=Dextromethorphan" }
    },
    {
      "src": "/psicoativos/heroina",
      "status": 301,
      "headers": { "Location": "/substance?name=Heroin" }
    },
    {
      "src": "/psicoativos/ibogaina",
      "status": 301,
      "headers": { "Location": "/substance?name=Ibogaine" }
    },
    {
      "src": "/psicoativos/ketamina",
      "status": 301,
      "headers": { "Location": "/substance?name=Ketamine" }
    },
    {
      "src": "/psicoativos/maconha",
      "status": 301,
      "headers": { "Location": "/substance?name=Cannabis" }
    },
    {
      "src": "/psicoativos/mescalina",
      "status": 301,
      "headers": {
        "Location": "/substance?name=Mescaline"
      }
    },
    {
      "src": "/psicoativos/metanfetamina",
      "status": 301,
      "headers": {
        "Location": "/substance?name=Methamphetamine"
      }
    },
    {
      "src": "/psicoativos/salvia",
      "status": 301,
      "headers": { "Location": "/substance?name=Salvinorin A" }
    },
    {
      "src": "/psicoativos/alcool",
      "status": 301,
      "headers": { "Location": "/substance?name=Alcohol" }
    },
    { "src": "/psicoativos/(?<name>[^/]+)", "dest": "/substance?name=$name" },
    {
      "src": "/sitemap.xml",
      "dest": "https://tripby-sitemap-generator.herokuapp.com/"
    }
  ]
}
